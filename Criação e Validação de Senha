<?php

echo "Crie uma senha \n";
    $senha = readline ();

do{ 
    echo "Faça login com a senha criada: \n";
        $senhaCorreta = readline ();
    
        if ($senhaCorreta != $senha){
            echo "Senha incorreta! Tente novamente. \n";
        }
} while ($senha != $senhaCorreta);
    echo "Acesso Liberado!";
